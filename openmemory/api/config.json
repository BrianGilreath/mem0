{
  "mem0": {
    "vector_store": {
      "provider": "qdrant",
      "config": {
        "collection_name": "openmemory",
        "host": "mem0_store",
        "port": 6333,
        "embedding_model_dims": 1024
      }
    },
    "llm": {
      "provider": "lmstudio",
      "config": {
        "model": "openai/gpt-oss-20b",
        "lmstudio_base_url": "http://host.docker.internal:1234/v1",
        "lmstudio_response_format": {
          "type": "json_schema",
          "json_schema": {
            "type": "object",
            "schema": {}
          }
        }
      }
    },
    "embedder": {
      "provider": "lmstudio",
      "config": {
        "model": "Qwen/Qwen3-Embedding-0.6B-GGUF",
        "lmstudio_base_url": "http://host.docker.internal:1234/v1",
        "embedding_dims": 1024
      }
    }
  }
}